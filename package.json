{
  "name": "cypress-ecommerce",
  "version": "1.0.0",
  "description": "É um projeto prático com o intuito de automatizar casos de testes do e-commerce",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:open:mobile":"cypress open --config viewportWidth=410 viewportHeight=860",
    "test": "cypress run",
    "test:mobile":"cypress run --config viewportWidth=410 viewportHeight=860",
    "test:cloud": "cypress run --record --key $CYPRESS_RECORD_KEY"
    },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Raiane-nepomuceno/cypress-ecommerce.git"
  },
  "keywords": [
    "Cypress Cloud",
    "GitHub Actions"
  ],
  "author": "Raiane Nepomuceno",
  "license": "MIT",
  "homepage": "https://github.com/Raiane-nepomuceno/cypress-ecommerce#readme",
  "devDependencies": {
    "cypress": "^13.12.0"
  },
  "dependencies": {
    "@faker-js/faker": "^9.6.0",
    "cpf-cnpj-validator": "^1.0.3"
  }
}
